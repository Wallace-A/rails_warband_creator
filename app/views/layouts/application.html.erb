<!DOCTYPE html>
<html>
  <head>
    <title>RailsWarbandCreator</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'application', "https://fonts.googleapis.com/css2?family=Cinzel:wght@400;700&display=swap"%>
    <%= stylesheet_link_tag 'application', "https://fonts.googleapis.com/css2?family=Grenze+Gotisch:wght@400;700&display=swap"%>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload', defer: true %>
  </head>

  <body>
    <div class="page-frame-content">
      <div class="page-frame-header">
            <%= render "layouts/navbar" %>
      </div>
      <div class="page-frame-body">
      <div class="container-main">
        <%= yield %>
      </div>
      </div>
      <div class="page-frame-footer">
          <%= render "layouts/footer" %>
      </div>
    </div>
    
    
  </body>
</html>
